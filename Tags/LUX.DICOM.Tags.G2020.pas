     Add( $0111, TKindVR.vrSQ, 'Basic Color Image Sequence' );
     Add( $0130, TKindVR.vrSQ, 'Referenced Image Overlay Box Sequence' );
     Add( $0140, TKindVR.vrSQ, 'Referenced VOI LUT Box Sequence' );
end;

